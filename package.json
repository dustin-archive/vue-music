{
  "name": "vue-music",
  "version": "0.0.1",
  "description": "A music list using vue.",
  "main": "index.js",
  "scripts": {
    "pug": "pug -P src/ -o out/",
    "sass": "node-sass src/ -o out/ --output-style expanded",
    "watchify": "watchify src/index.js -o out/index.js",
    "build": "npm run pug; npm run sass",
    "start": "browser-sync start --no-notify --no-ui --server --serveStatic out --files 'out/*.+(html|css)' --port 3000",
    "test": "npm run build; npm run start"
  },
  "author": "Preston Kyles",
  "license": "MIT",
  "dependencies": {
    "browser-sync": "^2.18.1",
    "browserify": "^13.1.1",
    "node-sass": "^3.13.0",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "vue": "^2.0.7",
    "vue-router": "^2.0.3",
    "watchify": "^3.7.0"
  }
}
